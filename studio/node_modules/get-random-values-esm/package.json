{
  "name": "get-random-values-esm",
  "version": "1.0.0",
  "description": "A wrapper that rebundles [`get-random-values`](https://www.npmjs.com/package/get-random-values) into ESM, so you can use it in your `vite`, `skypack`, or wherever you need ESM.",
  "keywords": [
    "crypto",
    "get-random-values",
    "getRandomValues",
    "webcrypto"
  ],
  "homepage": "https://github.com/stipsan/get-random-values-esm#readme",
  "bugs": {
    "url": "https://github.com/stipsan/get-random-values-esm/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/stipsan/get-random-values-esm.git"
  },
  "license": "MIT",
  "author": "Cody Olsen <stipsan@gmail.com>",
  "sideEffects": false,
  "exports": {
    ".": {
      "import": "./index.mjs",
      "browser": "./index.mjs",
      "deno": "./index.mjs",
      "node": "./index.cjs",
      "default": "./index.js"
    }
  },
  "main": "./index.js",
  "module": "./index.mjs",
  "browser": "./index.mjs",
  "types": "./index.d.ts",
  "files": [
    "index.js",
    "index.mjs",
    "index.d.ts",
    "index.cjs"
  ],
  "scripts": {
    "format": "prettier . --write",
    "prepublishOnly": "npm test",
    "test": "package-check && prettier . --check"
  },
  "prettier": {
    "semi": false,
    "singleQuote": true
  },
  "dependencies": {
    "get-random-values": "^1.2.2"
  },
  "devDependencies": {
    "@skypack/package-check": "^0.2.2",
    "prettier": "^2.6.2",
    "prettier-plugin-packagejson": "^2.2.18"
  }
}
